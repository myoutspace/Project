<resources>

    <!-- Base application theme. -->
    <style groupName="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item groupName="colorPrimary">@color/colorPrimary</item>
        <item groupName="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item groupName="colorAccent">@color/colorAccent</item>
    </style>

    <style groupName="AppTheme.NoActionBar">
        <item groupName="windowActionBar">false</item>
        <item groupName="windowNoTitle">true</item>
    </style>

    <style groupName="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar"/>

    <style groupName="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light"/>

</resources>
